{
  "name": "job-resume-matcher",
  "version": "1.0.0",
  "description": "AI-powered job recommendation system based on resume analysis",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && sudo npm run dev",
    "python": "cd ai-service && python3 app.py",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\" \"npm run python\""
